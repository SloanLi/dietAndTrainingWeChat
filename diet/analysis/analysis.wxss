/* diet/analysis/analysis.wxss */
@import '../index/index.wxss';
.shadow{
  padding: 20rpx 40rpx;
  margin: 0 auto;
 
}
.rate{
  width: 80%;
}
.rate_box,.process_box{
  display: flex;
  justify-content: space-around;
}
.diet_detail_item{
  text-align: center;
}
.process_item{
  width:24.5%;
  padding-top: 20rpx;
}
.process{
  height: 10rpx;
  border-radius: 20rpx;
  background: #bbbbbb;
  overflow: hidden;
  position: relative;
  text-align: left;
}
.process_span{
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  border-radius: 10rpx;

  display: inline-block;
  background:#5c6bc0;
}
.process_text{
  margin-top: 10rpx;
  font-weight: normal;
}
.process_active{
  font-weight: bolder;
  font-size: 32rpx;
}
.rate_item{
  width: 33%;

}
.rate_item_title{
  margin-bottom: 20rpx;
  display: block;
}
 .rate_item_text{
  color:#bbbbbb;
  font-size: 28rpx;
  line-height: 50rpx;
  display: block;
}
.echarts_box {
  margin: 0 auto;
  width: 100%;
  height: 400rpx;
}
ec-canvas {
  width: 200rpx;
  height: 200rpx;
}